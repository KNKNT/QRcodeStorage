<Application x:Class="QRcodeStorage.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QRcodeStorage"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/Palette.xaml"/>
                <ResourceDictionary Source="/Style/Buttons.xaml"/>
                <ResourceDictionary Source="/Style/DataGrid.xaml"/>
                <ResourceDictionary Source="/Style/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
