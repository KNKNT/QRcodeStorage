<Application x:Class="QRcodeStorage.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QRcodeStorage"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/Palette.xaml"/>
                <ResourceDictionary Source="/Style/MenuRadioButton.xaml"/>
                <ResourceDictionary Source="/Style/ToggleButton.xaml"/>
                <ResourceDictionary Source="/Style/CancelButton.xaml"/>
                <ResourceDictionary Source="/Style/UpdateButton.xaml"/>
                <ResourceDictionary Source="/Style/AcceptButton.xaml"/>
                <ResourceDictionary Source="/Style/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
