<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Foreground" Color="#2c3e50"/>
    <SolidColorBrush x:Key="Background" Color="#F6F7FB"/>
    <SolidColorBrush x:Key="PageForeground" Color="#758391"/>
    <SolidColorBrush x:Key="Accent" Color="#34495e"/>
    
    <!--MenuRadioBotton-->
    <SolidColorBrush x:Key="Active" Color="#48627d"/>
    <SolidColorBrush x:Key="DefaultForeground" Color="#9badbf"/>
    
    <!--SysBottons-->
    <SolidColorBrush x:Key="Exit" Color="#CE636B"/>
    <SolidColorBrush x:Key="Other" Color="#435d78"/>


</ResourceDictionary>